config_version=5

[application]

config/name="GGJ2025"
run/main_scene="res://Scenes/main_menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
boot_splash/image="res://Art/MainMenu/MainMenuClean.png"
config/icon="res://icon.svg"

[autoload]

