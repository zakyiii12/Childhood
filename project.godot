"Moment-3": "res://Dialogic/Timelines/Phase 1/Scene 1/Moment-3.dtl"
}
variables={
"result": false
}
layout/style_list=["res://Dialogic/DialogicStyles/VisualNovel.tres"]
layout/default_style="res://Dialogic/DialogicStyles/VisualNovel.tres"
glossary/default_case_sensitive=true
extensions_folder="res://addons/dialogic_additions"
text/letter_speed=0.05
text/initial_text_reveal_skippable=true
text/text_reveal_skip_delay=0.1
text/advance_delay=0.1
text/autoadvance_per_character_delay=0
text/autoadvance_ignored_characters_enabled=true
audio/max_channels=4.0
animations/join_default_length=0.5
animations/join_default_wait=true
animations/leave_default_length=0.5
animations/leave_default_wait=true
animations/cross_fade_default_length=0.5
choices/autofocus_first=true
choices/delay=0.2
choices/reveal_delay=0.5
choices/reveal_by_input=false
save/autosave_delay=60.0
save/encryption_on_exports_only=true
text/autopauses={}
text/autoadvance_enabled=true
text/autoadvance_fixed_delay=1.2
text/autoadvance_per_word_delay=0.005
history/simple_history_enabled=true
history/full_history_enabled=true
history/visited_event_history_enabled=true
history/simple_history_save=true
history/full_history_save=true
history/save_on_autosave=true
history/save_on_save=true
translation/enabled=false
translation/original_locale="en"
translation/file_mode=1
translation/translation_folder=""
translation/save_mode=0
translation/add_separator=false
text/autocolor_names=false

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/resizable=false
window/size/no_focus=true
window/stretch/mode="viewport"

[editor_plugins]

enabled=PackedStringArray("res://addons/dialogic/plugin.cfg", "res://addons/midi/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "red",
"res://Dialogic/": "green",
"res://Dialogic/Character/": "green",
"res://Dialogic/Timelines/Phase 1/": "red",
"res://Dialogic/Timelines/Phase 2/": "yellow",
"res://Scenes/": "blue",
"res://Script/": "gray",
"res://Sound/": "purple",
"res://fonts-master/": "pink"
}

